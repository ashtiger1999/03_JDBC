package edu.kh.prc;

import java.util.Scanner;

public class ggd {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in); 
		
		System.out.print("num 1 : ");
		int num = sc.nextInt(); 
		
		System.out.print("num 2 : ");
		int col = sc.nextInt();
		
		int start = 1;
		
		int row = num;		
		
		for(int i=1; i<=9; i++) {
			for(int e=start; e<=col; e++) {
				System.out.printf("%d X %d = %2d     ",e,i,i*e);
				row+=1;
			}
			System.out.println();
		}

	}
}