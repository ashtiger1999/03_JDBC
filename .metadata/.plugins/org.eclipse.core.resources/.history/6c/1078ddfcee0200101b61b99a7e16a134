package edu.kh.prc;

import java.util.Scanner;

public class ggd {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in); 
		
		System.out.print("num : ");
		int num = sc.nextInt(); 
		
		System.out.print("col : ");
		int col = sc.nextInt();
		
		int start = 1;
		
		int row = num/col;		
		
		for(int d=1; d<=row; d++) {
			for(int i=1; i<=9; i++) {
				for(int e=start; e<=start+col-1; e++) {
					System.out.printf("%d X %d = %2d     ",e,i,i*e);
				}
				System.out.println();
			}
			start+=col;
		}
	}
}