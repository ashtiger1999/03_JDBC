package edu.kh.prc;

import java.util.Scanner;

public class ggd {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in); 
		
		System.out.print("dan : ");
		int dan = sc.nextInt(); 
		
		System.out.print("col : ");
		int col = sc.nextInt();
		
		int row = 0;
		
		while(dan>0) {
			for(int times=1; times<=9; times++) {
				for(int i = row*col+1; i<col; i++ ) {
					System.out.printf("%2d X %2d = %2d",i,times,i*times);
				}
				System.out.println();
			}
			dan-=col;
		}
	}
}