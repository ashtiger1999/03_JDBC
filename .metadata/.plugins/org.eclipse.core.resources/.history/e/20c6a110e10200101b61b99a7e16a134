package edu.kh.prc;

import java.util.Scanner;

public class ggd {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in); 
		
		System.out.print("num 1 : ");
		int num1 = sc.nextInt(); 
		
		System.out.print("num 2 : ");
		int num2 = sc.nextInt();
		
		int row = num1;		
		
		for(int i=1; i<=9; i++) {
			for(int e=1; e<=num2; e++) {
				System.out.printf("%d X %d = %2d     ",row,i,i*row);
				row+=1;
			}
			System.out.println();
			row=num1;
		}
		
		System.out.println();
		
		for(int i=1; i<=9; i++) {
			for(int e=row+num2; e<=9; e++) {
				System.out.printf("%d X %d = %2d     ",e,i,i*e);
				row+=1;
			}
			System.out.println();
			row=num1;
		}
	}
}